<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dels.dao.orcl.BaseDataOrclMapper">

    <insert id="insertBaseData" parameterType="com.dels.model.monitor.BaseDataRegion">
        insert into dim_areadivide_test(xzqhdm,xzqhqc,sjdm,sjmc,qydm,qymc,qyjbdm,qyjbmc)
        values(#{XZQHDM},#{XZQHQC},#{SJDM},#{SJMC},#{QYDM},#{QYMC},#{QYJBDM},#{QYJBMC})
    </insert>

    <update id="updateBaseData" parameterType="com.dels.model.monitor.BaseDataRegion">
        update dim_areadivide set
          qymc=#{QYMC} ,
          xzqhqc=sjmc||#{QYMC}
        where xzqhdm=#{XZQHDM}
    </update>

    <delete id="deleteBaseData" parameterType="String">
        delete from dim_areadivide
        where xzqhdm=#{XZQHDM}
    </delete>

</mapper>